<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tesis.tse.tse_instructivojrv.ActivityMain">

    <Button
        android:id="@+id/button_manual_paso_a_paso"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:background="@color/color_panteone2"
        android:onClick="onClickButtonManual"
        android:paddingLeft="80dp"
        android:text=""
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_agenda"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:background="@color/color_panteone"
        android:onClick="onClickButtonAgenda"
        android:paddingLeft="80dp"
        android:text=""
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button_manual_paso_a_paso" />

    <Button
        android:id="@+id/button_aula_virtual"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:background="@color/color_panteone2"
        android:onClick="onClickButtonAulaVirtual"
        android:paddingLeft="80dp"
        android:text=""
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_agenda" />

    <Button
        android:id="@+id/button_web_tse"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:background="@color/color_panteone"
        android:onClick="onClickButtonWebTSE"
        android:paddingLeft="80dp"
        android:text=""
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_aula_virtual" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_actualizar_app"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginBottom="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:clickable="true"
        app:backgroundTint="@color/color_panteone"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.924"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/ico_actualizar_apk" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_acerca_de"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginBottom="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:clickable="true"
        android:onClick="onClickButtonAcercade"
        app:backgroundTint="@color/color_panteone2"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.681"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/ico_acerca_de_2" />

    <ImageView
        android:id="@+id/imageView_paso_paso"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:forceHasOverlappingRendering="false"
        android:translationZ="50dp"
        android:visibility="visible"
        android:layout_marginStart="28dp"
        tools:layout_editor_absoluteY="0dp"
        android:layout_marginLeft="38dp"
        app:layout_constraintLeft_toLeftOf="@+id/button_manual_paso_a_paso" />

    <ImageView
        android:id="@+id/imageView_agenda"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:translationZ="50dp"
        android:layout_marginStart="28dp"
        tools:layout_editor_absoluteY="116dp"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="@+id/button_agenda" />

    <ImageView
        android:id="@+id/imageView_aula_virtual"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:translationZ="50dp"
        app:srcCompat="@android:drawable/ic_dialog_dialer"
        android:layout_marginStart="28dp"
        tools:layout_editor_absoluteY="238dp"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="@+id/button_aula_virtual" />

    <ImageView
        android:id="@+id/imageView_sitio_web_tse"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:translationZ="50dp"
        app:srcCompat="@android:drawable/ic_dialog_alert"
        android:layout_marginStart="28dp"
        tools:layout_editor_absoluteY="346dp"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="@+id/button_web_tse" />

    <ImageView
        android:id="@+id/imageView_logo_tse"
        android:layout_width="137dp"
        android:layout_height="71dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/logo_tse"
        android:layout_marginStart="16dp" />

</android.support.constraint.ConstraintLayout>
