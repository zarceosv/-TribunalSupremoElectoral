<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tesis.tse.tse_instructivojrv.ActivityMain">

    <Button
        android:id="@+id/button_manual_paso_a_paso"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:background="@color/color_panteone2"
        android:onClick="pasoPaso"
        android:paddingLeft="80dp"
        android:text="@string/string_paso_paso"
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/button_agenda"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:layout_marginTop="115dp"
        android:background="@color/color_panteone"
        android:paddingLeft="80dp"
        android:text="@string/string_agenda"
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_manual_paso_a_paso"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/button_aula_virtual"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:layout_marginTop="230dp"
        android:background="@color/color_panteone2"
        android:text="@string/string_aula_virtual"
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingLeft="80dp"
        android:onClick="onClickButtonAulaVirtual"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/button_web_tse"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:layout_marginTop="345dp"
        android:background="@color/color_panteone"
        android:onClick="onClickButtonWebTSE"
        android:paddingLeft="80dp"
        android:text="@string/string_tse"
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_actualizar_app"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginBottom="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:clickable="true"
        app:backgroundTint="@color/color_panteone"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.924"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@android:drawable/stat_sys_download" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton_acerca_de"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginBottom="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:clickable="true"
        app:backgroundTint="@color/color_panteone2"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.681"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@android:drawable/stat_notify_error" />

    <ImageView
        android:id="@+id/imageView_paso_paso"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="38dp"
        android:forceHasOverlappingRendering="false"
        android:translationZ="50dp"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="@+id/button_manual_paso_a_paso"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_dialog_map" />

    <ImageView
        android:id="@+id/imageView_agenda"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="154dp"
        android:translationZ="50dp"
        app:layout_constraintLeft_toLeftOf="@+id/button_agenda"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_dialog_email"
        android:layout_marginStart="28dp" />

    <ImageView
        android:id="@+id/imageView_aula_virtual"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:translationZ="50dp"
        app:srcCompat="@android:drawable/ic_dialog_dialer"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="269dp"
        android:layout_marginStart="28dp" />

    <ImageView
        android:id="@+id/imageView_sitio_web_tse"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:translationZ="50dp"
        app:srcCompat="@android:drawable/ic_dialog_alert"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="389dp"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="28dp" />

    <ImageView
        android:id="@+id/imageView_logo_tse"
        android:layout_width="137dp"
        android:layout_height="71dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/logo_tse"
        android:layout_marginStart="16dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="20dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="20dp" />

</android.support.constraint.ConstraintLayout>
